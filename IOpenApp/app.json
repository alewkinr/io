{
  "expo": {
    "name": "IOpenApp",
    "slug": "IOpenApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/IO-min.png",
    "splash": {
      "image": "./assets/launch.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/IO-min.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/IO-min.png"
    }
  }
}
